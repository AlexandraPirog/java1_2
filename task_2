//Task_2.
//Вводится число n, затем n чисел целых, по одному на каждой строке. 
//Затем вводится число, на которое нужно умножить все введённые выше числа. 
//Выведите на экран результат умножения построчнo.

public class program {
public class Main {
    public static void main(String[] args) {
        Scanner iScanner = new Scanner(System.in);
        System.out.println("Введите количество чисел в массиве: ");
        int m = iScanner.nextInt();
        System.out.println("Введите число, на которое хотите умножить: ");
        int l = iScanner.nextInt();
        //System.out.println(m);
        for (int i = 0; i < m; i++){
            System.out.println("введите число, которое хотите умножить ");
            int n = iScanner.nextInt();
            System.out.println("результат умножения: ");
            System.out.println(n * l);
        }
        
    }

}
